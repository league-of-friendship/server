extends ../components/base.pug

block title
  title #{title} : Division inactive

block content
  div.columns
    div.column
      h3.title.is-3.has-icon
        span.icon.is-medium
          i.fa.fa-exclamation-triangle
        span Division inactive
  div.columns
    div.column
      if error
        span= error
      else
        div Registration unavailable for this division.
